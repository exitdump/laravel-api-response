{
    "name": "exitdump/laravel-api-response",
    "description": "A Laravel package for standardized API JSON response formatting",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Jahid Hasan",
        "email": "jahidorjahid@gmail.com"
      }
    ],
    "autoload": {
      "psr-4": {
        "Jahid\\ApiResponse\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Exitdump\\ApiResponse\\ApiResponseServiceProvider"
        ]
      }
    },
    "require": {
      "php": ">=8.0",
      "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
